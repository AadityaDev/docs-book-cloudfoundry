<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title %> | Cloud Foundry Docs</title>

    <!-- Templates can insert head content here (like meta redirects) -->
    <%= yield_content(:head_content) %>

    <link href="/stylesheets/master.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/style.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/breadcrumbs.css" media="screen" rel="stylesheet" type="text/css" />
    <script src="/javascripts/all.js" type="text/javascript"></script>
    <link href='http://cloudfoundry.org/images/favicon.ico' rel='shortcut icon'>

    <script>
        if (window.location.host === '<%= production_host %>') {
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-22181585-2', 'cloudfoundry.org');
          ga('send', 'pageview');
        }
    </script>
  </head>

  <body class="<%= page_classes %>">
    <img id="bg" src="/images/bg-plain-x.jpg">
    <div id='non_footer'>
      <div id='content'>
        <header class="header-home">
          <div class="home-links js-bar-links">
            <div class="btn-menu" data-behavior="MenuMobile"></div>
            <div class="header-item">
              <a href="<%=vars.support_url%>" target="_blank">Need Support?</a>
            </div>
            <div class="header-item searchbar js-searchbar">
              <a class="search-icon" data-behavior="Search"></a>
              <div class="search-input">
                <!-- Google CSE Search Box -->
                <div class="search-input-inner" id="docs-search">
                    <gcse:search></gcse:search>
                </div>
              </div>
            </div>
          </div>
          <h1 class="title-flashy">
           <img src="<%=vars.title_image%>"> <br> <%=vars.title%>
          </h1>
          <h2 class="title-intro">
            <%=vars.hero_text%>
          </h2>
        </header>
<!--             <div class="text-center" id='orgheaderbg'>
              <div id='orglogo'>
                <a href="http://www.cloudfoundry.org"><img src="/images/logo_org.png"></a>
              </div>
            </div> -->
        <div class='container-full'>
          <div id='organization_bar'>
            <div class='navbar'>
              <ul class='nav'>
                <li id='icon-home-li'>
                    <a id='icon-home-link' href='http://www.cloudfoundry.org'>
                        <img id='icon-home' src='http://blog.cloudfoundry.org/wp-content/static/cforg/img/icon-home.png'>
                    </a>
                </li>
                <li>
                    <a id='about' href='http://www.cloudfoundry.org/about/index.html'>About</a>
                </li>
                <li>
                    <a id='learn' href='http://www.cloudfoundry.org/learn/index.html'>Learn</a>
                </li>
                <li>
                    <a id='docs' href='http://docs.cloudfoundry.org/'>Documentation</a>
                </li>
                <li>
                    <a id='get-involved' href='http://www.cloudfoundry.org/get-in/index.html'>Get Involved</a>
                </li>
                <li>
                    <a href='http://blog.cloudfoundry.org'>Blog</a>
                </li>
                <li id="wiki">
                    <a href='https://github.com/cloudfoundry-community/cf-docs-contrib/wiki' target='_blank'>Wiki</a>
                </li>
              </ul>
            </div><!-- End of navbar -->
          </div><!-- End of organization_bar -->
          <div class='content_inner'>
            <a class="forkme" href="https://github.com/cloudfoundry/?query=docs" target="_blank">
              <img alt="Fork me on GitHub" src="/images/ribbon_forkme.png">
            </a>

            <div id='google-cse'>
              <script>
                (function() {
                  var cx = '005804290371689664731:9jhxguhn8dy';
                  var gcse = document.createElement('script');
                  gcse.type = 'text/javascript';
                  gcse.async = true;
                  gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
                      '//www.google.com/cse/cse.js?cx=' + cx;
                  var s = document.getElementsByTagName('script')[0];
                  s.parentNode.insertBefore(gcse, s);
                })();
              </script>
              <gcse:search></gcse:search>
            </div>


            <br clear="left">

      <% unless current_page.url == "/" %>

              <%= breadcrumbs %>
              <br clear="left">
              <h3 class="title-container"><%= current_page.data.title %></h3>

              <%= quick_links %>



      <% end %>
      <%= yield %>

          </div><!-- End of content_inner -->
        </div><!-- End of container-full -->
      </div><!-- End of content -->
    </div><!-- End of non_footer -->
    <div id='footer'>
      <div id='container-footer'>
        <div class='bg_social'>
          <ul class='social'>
            <li>
            <a href='http://twitter.com/cloudfoundry' target='_blank'>
            <img alt='Twitter' src='/images/twitter_large.png'>
            </a>
            </li>
            <li>
            <a href='https://plus.google.com/114245065906861739918/posts' target='_blank'>
            <img alt='Google+' src='/images/gplus_large.png'>
            </a>
            </li>
            <li>
            <a href='http://facebook.com/cloudfoundry' target='_blank'>
            <img alt='Facebook' src='/images/facebook_large.png'>
            </a>
            </li>
            <li>
            <a href='http://youtube.com/cloudfoundry' target='_blank'>
            <img alt='Youtube' src='/images/youtube_large.png'>
            </a>
            </li>
          </ul>
        </div><!-- End of bg_social -->
        <ul class='links'>
                <li>
                    <a href='http://www.cloudfoundry.org/about/index.html'>About</a>
                </li>
                <li class='disc'>•</li>
                <li>
                    <a href='http://docs.cloudfoundry.com/'>Documentation</a>
                </li>
                <li class='disc'>•</li>
                <li>
                    <a href="https://github.com/cloudfoundry" target="_blank">Source Code</a>
                </li>
                <li class='disc'>•</li>
                <li>
                    <a href='https://groups.google.com/a/cloudfoundry.org/forum/#!browse' target="_blank">Mailing Lists</a>
                </li>
                <li class='disc'>•</li>
                <li>
                    <a href='http://blog.cloudfoundry.org/'>Blog</a>
                </li>
                <li class='disc'>•</li>
                <li>
                     <a href='https://github.com/cloudfoundry-community/cf-docs-contrib/wiki' target="_blank">Wiki</a>
                </li>
                <li class='disc'>•</li>
                <li>
                     <a href='http://cloudfoundry.org/dojo/index.html'>The Dojo</a>
                </li>
        </ul>
        <div class='copyright'>
          <div class='hr-horizontal-dark'></div>
          <br>
          <p>
            &copy;
            <script>
              //<![CDATA[
                var d = new Date();
                document.write(d.getFullYear());
              //]]>
            </script>
            <a href='http://gopivotal.com' target='_blank'>Pivotal Software</a> Inc. All Rights Reserved.
          </p>
        </div><!-- End of copyright -->
      </div><!-- End of container-footer -->
    </div><!-- End of footer -->
    <script type="text/javascript">
      (function() {
        var didInit = false;
        function initMunchkin() {
          if(didInit === false) {
            didInit = true;
            Munchkin.init('625-IUJ-009');
          }
        }
        var s = document.createElement('script');
        s.type = 'text/javascript';
        s.async = true;
        s.src = document.location.protocol + '//munchkin.marketo.net/munchkin.js';
        s.onreadystatechange = function() {
          if (this.readyState == 'complete' || this.readyState == 'loaded') {
            initMunchkin();
          }
        };
        s.onload = initMunchkin;
        document.getElementsByTagName('head')[0].appendChild(s);
      })();
    </script>
  </body>
</html>
